table ip filter {
    chain input {
        type filter hook input priority 0
        policy {{ nftables_ipv4_policy_input }}
{% for rule in nftables_ipv4_input_rules %}
        {{ rule }}
{% endfor %}
    }
    chain output {
        type filter hook output priority 0
        policy {{ nftables_ipv4_policy_output }}
{% for rule in nftables_ipv4_output_rules %}
        {{ rule }}
{% endfor %}
    }
    chain forward {
        type filter hook forward priority 0
        policy {{ nftables_ipv4_policy_forward }}
{% for rule in nftables_ipv4_forward_rules %}
        {{ rule }}
{% endfor %}
    }
}
